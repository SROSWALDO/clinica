// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Paciente {
  id Int @id @default(autoincrement())
  fecha DateTime
  hora String
  nombre String
  telefono String
  consulta String
  doctor String
  radiografias Int
  ambulancia Boolean
  ingresos Int
  egresos Int
  createdAt DateTime @default(now())
}